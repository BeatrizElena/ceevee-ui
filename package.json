{
  "name": "ceevee-ui",
  "version": "0.0.0-development",
  "private": true,
  "description": "Small description for ceevee-ui goes here",
  "license": "MIT",
  "author": "",
  "directories": {
    "doc": "doc",
    "test": "tests"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/randallmorey/ceevee-ui.git"
  },
  "scripts": {
    "build": "ember build",
    "clean:coverage": "rm -rf coverage_*",
    "precommit": "npm test",
    "commit": "git-cz",
    "license:check": "node-license-validator --allow-licenses Apache Apache-2.0 \"Apache License\" BSD BSD-2-Clause BSD-3-Clause BSD-like CC-BY-3.0 CC-BY-4.0 CC0-1.0 ISC MIT MIT/X11 \"Public Domain\" Unlicense",
    "license:report": "npm run license:check -- -v",
    "lint:js": "eslint ./*.js app blueprints config lib server tests",
    "start": "ember serve",
    "pretest": "npm run clean:coverage && npm run license:check && npm run lint:js",
    "test": "COVERAGE=true ember exam --split=1 --parallel --random",
    "posttest": "ember coverage-merge && npm run clean:coverage",
    "travis-deploy-once": "travis-deploy-once",
    "semantic-release": "semantic-release"
  },
  "devDependencies": {
    "broccoli-asset-rev": "^2.4.5",
    "cz-conventional-changelog": "^2.1.0",
    "ember-ajax": "^3.0.0",
    "ember-cli": "~3.1.4",
    "ember-cli-app-version": "^3.0.0",
    "ember-cli-babel": "^6.6.0",
    "ember-cli-code-coverage": "^0.4.2",
    "ember-cli-dependency-checker": "^2.0.0",
    "ember-cli-eslint": "^4.2.1",
    "ember-cli-htmlbars": "^2.0.1",
    "ember-cli-htmlbars-inline-precompile": "^1.0.0",
    "ember-cli-inject-live-reload": "^1.4.1",
    "ember-cli-qunit": "^4.1.1",
    "ember-cli-shims": "^1.2.0",
    "ember-cli-sri": "^2.1.0",
    "ember-cli-uglify": "^2.0.0",
    "ember-data": "~3.1.0",
    "ember-exam": "^1.0.0",
    "ember-export-application-global": "^2.0.0",
    "ember-load-initializers": "^1.0.0",
    "ember-maybe-import-regenerator": "^0.1.6",
    "ember-model-validator": "^2.18.0",
    "ember-resolver": "^4.0.0",
    "ember-source": "~3.1.0",
    "ember-welcome-page": "^3.0.0",
    "eslint-plugin-ember": "^5.0.0",
    "loader.js": "^4.2.3",
    "node-license-validator": "^1.3.0",
    "semantic-release": "^15.5.0",
    "travis-deploy-once": "^5.0.0"
  },
  "engines": {
    "node": "^9.3"
  },
  "config": {
    "commitizen": {
      "path": "./node_modules/cz-conventional-changelog"
    }
  }
}
